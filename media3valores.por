programa {
  funcao inicio() 
  
  {
    //crie um algoritmo para calcular a media de tres numeros

     //definindo variaveis
     inteiro num1, num2, num3
     real media

     escreva("digite um numero:")
     leia(num1)

     escreva("digite outro numero:")
     leia(num2)
    
    escreva("digite mais outro numero:")
    leia(num3)

    media=(num1+num2+num3)/3
    escreva(media)
  
  }
}
